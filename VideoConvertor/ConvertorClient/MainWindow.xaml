<Window x:Class="ConvertorClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel Orientation="Horizontal" >
            <TextBox x:Name="txtBox" Width="300" Height="30"></TextBox>
            <Button x:Name="btn" Height="30" Content="Import" Click="btn_Click" Width="100"></Button>
            <Button x:Name="btn1" Height="30" Click="btn1_Click" Content="Process"  Width="100"></Button>
        </StackPanel>
        
    </Grid>
</Window>
